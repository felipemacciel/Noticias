{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "noticias-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/noticias/NoticiasWebPart.js",
          "manifest": "./src/webparts/noticias/NoticiasWebPart.manifest.json"
        }
      ]
    },
    "noticias-pagina-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/noticiasPagina/NoticiasPaginaWebPart.js",
          "manifest": "./src/webparts/noticiasPagina/NoticiasPaginaWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "NoticiasWebPartStrings": "lib/webparts/noticias/loc/{locale}.js",
    "NoticiasPaginaWebPartStrings": "lib/webparts/noticiasPagina/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}